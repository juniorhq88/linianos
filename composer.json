{
    "name": "junior/php",
    "description": "Test Project for Linio application",
    "keywords": ["junior", "php"],
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=7.0"
            },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Linio\\": "src"         
        }
    },
    "autoload-dev": {
